@model IEnumerable<Student>
@*Generated from Custom Template.*@
@*Model namespace using directive should be added to _ViewImports.cshtml.*@

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>



    @if (Model is not null)
    {
        <p class="text-success">@Model.Count() record(s) found.</p>
    }
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FullNameOutput)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.UniversityExamRank)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CumulativeGpa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GradeOutput)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
		@foreach (var item in Model) {
				<tr>
					<td>
						@Html.DisplayFor(modelItem => item.FullNameOutput)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.UniversityExamRank)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.CumulativeGpa)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.GradeOutput)
					</td>
					<td>
						<a asp-action="Details" asp-route-id="@item.Id">Details</a>&nbsp;|&nbsp;
						<a asp-action="Edit" asp-route-id="@item.Id">Edit</a>&nbsp;|&nbsp;
						<a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
					</td>
				</tr>
		}
    </tbody>
</table>
</body>
</html>



