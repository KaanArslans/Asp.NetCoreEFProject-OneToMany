@model Student
@*Generated from Custom Template.*@
@*Model namespace using directive should be added to _ViewImports.cshtml.*@

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<body>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Student</h4>
    <hr />
    <dl class="row">
        <dt class="col-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class="col-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-2">
            @Html.DisplayNameFor(model => model.Surname)
        </dt>
        <dd class="col-10">
            @Html.DisplayFor(model => model.Surname)
        </dd>
        <dt class="col-2">
            @Html.DisplayNameFor(model => model.UniversityExamRank)
        </dt>
        <dd class="col-10">
            @Html.DisplayFor(model => model.UniversityExamRank)
        </dd>
        <dt class="col-2">
            @Html.DisplayNameFor(model => model.CumulativeGpa)
        </dt>
        <dd class="col-10">
            @Html.DisplayFor(model => model.CumulativeGpa)
        </dd>
        <dt class="col-2">
            @Html.DisplayNameFor(model => model.GradeOutput)
        </dt>
        <dd class="col-10">
            @Html.DisplayFor(model => model.GradeOutput)
        </dd>
    </dl>
    
    <form asp-action="Delete">
	@Html.AntiForgeryToken()
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Yes" class="btn btn-danger" />&nbsp;&nbsp;
        <a asp-action="Index" class="btn btn-success">No</a>
    </form>
</div>
</body>
</html>



